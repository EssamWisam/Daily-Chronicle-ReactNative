{
  "expo": {
    "name": "Daily Chronicle",
    "slug": "diary-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/real-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.essamos.quizzery"
    },
    "android": {
      "package": "com.essamos.diary",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/real-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
